#!/usr/bin/env bash

echo "Installing Brew Cask..."

brew tap caskroom/cask

cask_apps=(
  arduino
  atom
  dropbox
  fritzing
  github
  google-chrome
  hyper
  iterm2
  kite
  lastpass
  makerbot-print
  skype
  spotify
  telegram
  vuze
  whatsapp
)
# TODO: google drive

brew cask install "${cask_apps[@]}"
